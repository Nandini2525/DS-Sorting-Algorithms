class HelloWorld {
    public static void main(String[] args) {
        int arr[]={3,5,10,2,1};
        int j=0;
        while(j<arr.length){
        for(int i=0;i<arr.length-1;i++){
            if(arr[i]>arr[i+1]){
                int temp=arr[i];
                arr[i]=arr[i+1];
                arr[i+1]=temp;
            }
        }
        j++;
        }
        for(int i=0;i<arr.length;i++){
            System.out.println(arr[i]);
        }
    }
}
